      <th></th>
    </tr>
  </thead>
  <tbody>
    <% end_users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image, size:'100x100' %></td>
        <td><%= user.name %></td>
        <td>フォロー<%= user.followers.count %></td>
        <td>フォロワー<%= user.followings.count %></td>
        <td><%= link_to  "詳細", end_user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>