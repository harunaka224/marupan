<div class="search_form float-right">
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= f.select :model, options_for_select({"ユーザー"=>"end_user","投稿"=>"post"}) %>
    <%= f.select :method, options_for_select({"完全一致"=>"perfect","前方一致"=>"forward","後方一致"=>"backward","部分一致"=>"partial"}) %>
    <%= f.submit "検索", class: "btn btn-warning" %>
  <% end %>
</div>